{
  "name": "@gerhobbelt/babel-loader",
  "version": "8.0.0-1",
  "description": "babel module loader for webpack",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6.9"
  },
  "dependencies": {
    "find-cache-dir": "2.0.0",
    "loader-utils": "1.1.0",
    "mkdirp": "0.5.1",
    "util.promisify": "1.0.0"
  },
  "peerDependencies": {
    "@gerhobbelt/babel-core": "^7.0.0",
    "webpack": ">=2"
  },
  "devDependencies": {
    "@gerhobbelt/babel-cli": "7.0.0-beta.530006",
    "@gerhobbelt/babel-core": "7.0.0-beta.530006",
    "@gerhobbelt/babel-preset-env": "7.0.0-beta.530006",
    "ava": "0.25.0",
    "babel-eslint": "9.0.0",
    "babel-plugin-istanbul": "5.0.1",
    "babel-plugin-react-intl": "2.4.0",
    "cross-env": "5.2.0",
    "eslint": "5.4.0",
    "eslint-config-babel": "7.0.2",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-prettier": "2.6.2",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "nyc": "13.0.1",
    "prettier": "1.14.2",
    "react": "16.4.2",
    "react-intl": "2.4.0",
    "react-intl-webpack-plugin": "0.2.0",
    "rimraf": "2.6.2",
    "webpack": "4.17.1"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "build": "babel src/ --out-dir lib/",
    "format": "prettier --write --trailing-comma all 'src/**/*.js' 'test/**/*.test.js' 'test/helpers/*.js' && prettier --write --trailing-comma es5 'scripts/*.js'",
    "lint": "eslint src test",
    "precommit-orig": "lint-staged",
    "prepublish-orig": "npm run clean && npm run build",
    "preversion-orig": "npm test",
    "test": "npm run lint && cross-env BABEL_ENV=test npm run build && npm run test-only",
    "test-only": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babel/babel-loader.git"
  },
  "keywords": [
    "webpack",
    "loader",
    "babel",
    "es6",
    "transpiler",
    "module"
  ],
  "author": "Luis Couto <hello@luiscouto.pt>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/babel/babel-loader/issues"
  },
  "homepage": "https://github.com/babel/babel-loader",
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text",
      "json"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "ava": {
    "files": [
      "test/**/*.test.js",
      "!test/fixtures/**/*",
      "!test/helpers/**/*"
    ],
    "source": [
      "src/**/*.js"
    ]
  },
  "lint-staged": {
    "scripts/*.js": [
      "prettier --trailing-comma es5 --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --trailing-comma all --write",
      "git add"
    ],
    "test/**/*.test.js": [
      "prettier --trailing-comma all --write",
      "git add"
    ],
    "test/helpers/*.js": [
      "prettier --trailing-comma all --write",
      "git add"
    ],
    "package.json": [
      "node ./scripts/yarn-install.js",
      "git add yarn.lock"
    ]
  }
}
